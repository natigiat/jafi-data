  body.scanner
    nav.navbar.scannerNav.navbar-static-top(role='navigation')
      // Brand and toggle get grouped for better mobile display
      div
        button.navbar-toggle(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-8')
          span.sr-only Toggle navigation
          |                  
          span.icon-bar
          |                  
          span.icon-bar
          |                  
          span.icon-bar
        |               
        a(href='/')
          img.Navbar-brand(src='/images/4444.png')
      // Collect the nav links, forms, and other content for toggling
      #bs-example-navbar-collapse-8.collapse.navbar-collapse
        ul.nav.navbar-nav
          li.active.desktop
            a
              //- i(class="fa fa-desktop fa-2x")
              span.icon-desktop.fs1            
          li
            a.mobile
              //- i(class="fa fa-mobile fa-2x")
              span.icon-tablet.fs1         
          

          

         ul.nav.navbar-nav.navbar-right.publishNav
            li.drowB
              a <i class="fa fa-crop"></i>
            li.styleB
              a <i class="fa fa-file-image-o"></i>
            li.codeB 
              a <i class="fa fa-code"></i>
            li.preview 
              a Preview
            li
              a(data-toggle="modal" data-target="#myModal") Save
              div#myModal.modal.fade(tabindex='-1', class="modal newspaper unfold-3d" , role='dialog', aria-labelledby='myModalLabel' ,aria-hidden="true")
                .modal-dialog(role='document')
                  .modal-content 
                    .modal-body
                        .col-sm-12
                          .col-sm-10.pull-left
                            i(class="fa fa-database fa-4x saveProgect")
                            span.textProgectOne It's time to save your work
                            br 
                            span.textProgectTwo and to give your website a name
                          .col-sm-12
                            .col-sm-10
                              input.form-control(type='text' , name='project' , class="progectInput" , autocomplete="off", placeholder='Your site name')
                              div#search_bar
                            button.btn.btnProgect(type='button' class="col-sm-2") Success
                            .col-sm-10
                              div.progectRow http://landing/#{name}/#[b.progectName]
                          hr
                          .col-sm-12
                            ol
                              li You can change your site name later    

                              li your site is private and will only be shown to the world with your approved 
                                
                              li You can publish your site
            li
              a Publish 

            //-fileupload 
            div#upload.modal.fade(tabindex='-1', class="modal newspaper unfold-3d" , aria-labelledby='myModalLabel' ,aria-hidden="true")
                .modal-dialog(role='document')
                  .modal-header
                    .col-sm-9
                      h3 Responsive
                    .col-sm-3
                      button.close(type='button', data-dismiss='modal', aria-hidden='true') &times;
                    
                  .modal-content 
                    .modal-body
                        .col-sm-12
                            ul.nav.nav-tabs
                                li.active
                                    a(href='#tab1', data-toggle='tab') Tab 1
                                      
                                  li
                                    a(href='#tab2', data-toggle='tab') Tab 2
                                  
                                .tab-content
                                  #tab1.tab-pane.active
                                    p This modal was loaded in via ajax This modal was loaded in via ajaxThis modal was loaded in via ajaxThis modal was loaded in via ajax
                                    form(method='post', enctype='multipart/form-data', action='/scanner/upload') 
                                      input(type="file", name="onixfile", id="onixfile")    
                                      input(type='submit')
                                      
                                  #tab2.tab-pane
                                    p This is some other tab contentThis modal was loaded in via ajaxThis modal was loaded in via ajaxThis modal was loaded in via ajax
     


